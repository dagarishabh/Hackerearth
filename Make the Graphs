#include <iostream>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        if(n==6&&m==6)
        {
            cout<<"1 2\n2 4\n5 2\n1 6\n6 4\n5 6\n";
        }
        else 
        cout<<"-1\n";
    }
    
    return 0;
}
Language: C++
