#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    string a[26];
    int i,j,k=0;
    for(i=97;i<=122;i++)
    {
        a[k]="";
        for(j=0;j<s.length();j++)
        {
            if(s[j]==(char)i);
            else
            a[k]+=s[j];
        }
        k++;
    }
    sort(a,a+26);
    cout<<a[0]<<endl;
    return 0;
}
Language: C++14
