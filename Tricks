#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        long long int n,ans=0;
        cin>>n;
        while(n>1)
        {
            ans++;
            if(n%2)
            n--;
            else
            n/=2;
        }
        cout<<ans<<endl;
    }
    return 0;
}
