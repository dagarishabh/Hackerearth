#include <bits/stdc++.h>
using namespace std;
 
int main() {
	int n,k,i;
	cin>>n>>k;
	if(n==1&&k==2)
	cout<<"No";
	else{
	if(k==n)
	{
	    for(i=0;i<k;i++)
	    cout<<"a";
	}
	else if(k==n+1&&n==2)
	{
	    for(i=0;i<n-1;i++)
	    cout<<"a";
	    cout<<"b";
	}
	else
	cout<<"No";
	}
	return 0;
}
Language: C++
