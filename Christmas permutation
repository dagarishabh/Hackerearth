#include <bits/stdc++.h>
using namespace std;
 
int main() {
	int n;
	cin>>n;
	int i;
	if(n%2==1)
	{
	    for(i=0;i<=n/2;i++)
	    cout<<2*i+1<<" ";
	    for(i=n/2;i>=1;i--)
	    cout<<2*i<<" ";
	    
	}
	else
	{
	    for(i=0;i<n/2;i++)
	    cout<<2*i+1<<" ";
	    for(i=n/2;i>=1;i--)
	    cout<<2*i<<" ";
	    
	}
	return 0;
}
Language: C++
